{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "outDir": "src/static/js",
    "rootDir": "src/ts",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": [
    "src/ts/**/*"
  ]
}